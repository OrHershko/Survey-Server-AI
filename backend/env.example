# Test Environment Configuration
# test or development
NODE_ENV=test
PORT=5000

# Database Configuration (will use mongodb-memory-server)
MONGODB_URI=mongodb://localhost:27017/survey-server-test

# Authentication (test secrets)
JWT_SECRET=test-jwt-secret-key
JWT_EXPIRES_IN='1h'
JWT_REFRESH_EXPIRES_IN='7d'
REGISTRATION_SECRET=test-registration-code

# External Services (use mocks in tests)
OPENROUTER_API_KEY=test-api-key
USE_MOCK_LLM=true

# Logging
LOG_LEVEL=info 